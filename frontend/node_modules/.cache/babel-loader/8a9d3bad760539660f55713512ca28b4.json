   url: '/bilibili/auth/logout',\n    method: 'post'\n  })\n}\n\n// 获取用户信息\nexport function getUserInfo(userId) {\n  return bilibiliApi({\n    url: '/live/anchor/info',\n    method: 'get',\n    params: {\n      userId\n    },\n    needAuth: true\n  })\n}\n\n// 获取视频信息\nexport function getVideoInfo(bvid) {\n  return bilibiliApi({\n    url: '/video/info',\n    method: 'get',\n    params: {\n      bvid\n    },\n    needAuth: true\n  })\n}\n\n// 搜索视频\nexport function searchVideos(keyword, page = 1, pageSize = 20) {\n  return bilibiliApi({\n    url: '/video/search',\n    method: 'get',\n    params: {\n      keyword,\n      page,\n      pageSize\n    },\n    needAuth: true\n  })\n}"],"mappings":"AAAA,OAAOA,OAAO,MAAM,WAAW;AAC/B,OAAOC,WAAW,MAAM,8BAA8B;;AAEtD;AACA,OAAO,SAASC,cAAcA,CAAA,EAAG;EAC/B,OAAOF,OAAO,CAAC;IACbG,GAAG,EAAE,gCAAgC;IACrCC,MAAM,EAAE;EACV,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASC,gBAAgBA,CAACC,SAAS,EAAE;EAC1CC,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEF,SAAS,CAAC;EAC3C,OAAON,OAAO,CAAC;IACbG,GAAG,EAAE,4BAA4B;IACjCC,MAAM,EAAE,KAAK;IACbK,MAAM,EAAE;MACNH;IACF;EACF,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASI,kBAAkBA,CAAA,EAAG;EACnC,OAAOV,OAAO,CAAC;IACbG,GAAG,EAAE,wBAAwB;IAC7BC,MAAM,EAAE;EACV,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASO,gBAAgBA,CAAA,EAAG;EACjC,OAAOX,OAAO,CAAC;IACbG,GAAG,EAAE,uBAAuB;IAC5BC,MAAM,EAAE;EACV,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASQ,MAAMA,CAAA,EAAG;EACvB,OAAOZ,OAAO,CAAC;IACbG,GAAG,EAAE,uBAAuB;IAC5BC,MAAM,EAAE;EACV,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASS,WAAWA,CAACC,MAAM,EAAE;EAClC,OAAOb,WAAW,CAAC;IACjBE,GAAG,EAAE,mBAAmB;IACxBC,MAAM,EAAE,KAAK;IACbK,MAAM,EAAE;MACNK;IACF,CAAC;IACDC,QAAQ,EAAE;EACZ,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASC,YAAYA,CAACC,IAAI,EAAE;EACjC,OAAOhB,WAAW,CAAC;IACjBE,GAAG,EAAE,aAAa;IAClBC,MAAM,EAAE,KAAK;IACbK,MAAM,EAAE;MACNQ;IACF,CAAC;IACDF,QAAQ,EAAE;EACZ,CAAC,CAAC;AACJ;;AAEA;AACA,OAAO,SAASG,YAAYA,CAACC,OAAO,EAAEC,IAAI,GAAG,CAAC,EAAEC,QAAQ,GAAG,EAAE,EAAE;EAC7D,OAAOpB,WAAW,CAAC;IACjBE,GAAG,EAAE,eAAe;IACpBC,MAAM,EAAE,KAAK;IACbK,MAAM,EAAE;MACNU,OAAO;MACPC,IAAI;M