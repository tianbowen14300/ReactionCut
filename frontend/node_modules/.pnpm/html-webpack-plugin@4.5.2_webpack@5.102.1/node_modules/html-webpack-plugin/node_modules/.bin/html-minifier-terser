#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tbw/work/btProject/reaction-cut/frontend/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/Users/tbw/work/btProject/reaction-cut/frontend/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/Users/tbw/work/btProject/reaction-cut/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tbw/work/btProject/reaction-cut/frontend/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/Users/tbw/work/btProject/reaction-cut/frontend/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/Users/tbw/work/btProject/reaction-cut/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../html-minifier-terser@5.1.1/node_modules/html-minifier-terser/cli.js" "$@"
else
  exec node  "$basedir/../../../../../html-minifier-terser@5.1.1/node_modules/html-minifier-terser/cli.js" "$@"
fi
